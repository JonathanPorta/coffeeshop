http = require 'http'

module.exports = do ->
	import: (url)->
		console.log "Importing from a URL: " + url
		http.get url, (res)->
			res.on "data", (data)->
				console.log "Got a Response!"
				products = JSON.parse data
				console.log "Products: " + products.length
		.on 'error', (e)->
			console.log "Error!"
			console.log(e)
