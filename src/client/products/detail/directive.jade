.productDetailDirective
	.row
		.col-md-3
			.row
				img.img-thumbnail.product-detail-picture(width="200px", height="200px", src="data:image/png;base64,")
			.row.center-block
				img.img-thumbnail.product-detail-thumbnail(width="65px", height="65px", src="data:image/png;base64,")
				img.img-thumbnail.product-detail-thumbnail(width="65px", height="65px", src="data:image/png;base64,")
				img.img-thumbnail.product-detail-thumbnail(width="65px", height="65px", src="data:image/png;base64,")
		.col-md-6
			header
				h1.products-detail-header(ng-hide="product.name") No Product Name :(
				h1.products-detail-header(ng-show="product.name")
					|{{product.name}}
					small - {{product.partno}}
			section.row
				strong Description: 
				{{product.description}}
			footer
		.col-md-3
			.panel.panel-primary
				.panel-heading.product-detail-price-container
					h5.product-detail-price
						| $2.95
					p
						button.btn Add to cart
				.panel-body.text-center
					span.label.label-success In Stock!
				ul.list-group
					li.list-group-item.text-center
						strong
							|Manufacturer:
						p
							|Testy products, Inc.
					li.list-group-item.text-center
						strong
							|SKU: 
						|{{product.sku}}
	.row
		.col-md-12
			.panel.panel-primary
				.panel-heading
					h5.panel-title Related Products
				.panel-body.text-center


